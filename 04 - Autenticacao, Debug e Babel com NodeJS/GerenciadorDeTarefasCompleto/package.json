{
  "name": "gerenciador-de-tarefas-completo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s clean build serve",
    "clean": "del -f dist",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "build": "babel . --delete-dir-on-start -d dist --copy-files --source-maps --ignore node_modules,dist,public,package.json,package-lock.json,yarn.lock,*.sqlite",
    "dev": "cross-env NODE_ENV=development DEBUG=http:* nodemon index.js --exec babel-node"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "3.1.0",
    "express": "4.16.2",
    "express-validator": "4.3.0",
    "jsonwebtoken": "8.1.0",
    "moment": "2.19.4",
    "morgan": "1.9.0",
    "mysql2": "1.5.1",
    "sequelize": "4.28.3",
    "url-join": "2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "copyfiles": "1.2.0",
    "cross-env": "5.1.1",
    "del-cli": "^1.1.0",
    "nodemon": "1.13.3",
    "npm-run-all": "4.1.2",
    "sequelize-cli": "3.2.0",
    "sqlite3": "3.1.13"
  }
}
